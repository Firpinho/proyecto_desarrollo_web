=begin 
RewriteEngine on

# php -- Páginas Princioales_
	RewriteRule ^Nosotros /pages/nosotros.min.html
	RewriteRule ^Inversión_retiro /pages/inversion_retiro.min.html
	RewriteRule ^Viviendas_tuteladas /pages/apartamentos_premium.min.html
	RewriteRule ^Llave_en_mano /pages/llave_en_mano.min.html
	RewriteRule ^Presupuestos /pages/presupuestos.min.html
